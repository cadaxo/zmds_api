     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Ein View, Sperrobjekt oder Matchcode-Objekt basiert auf einer oder</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>mehrerer Basistabellen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Viewfelder bzw. Matchcode- und Sperrobjektfelder sowie</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Selektionsbedingungen dürfen sich nur auf Felder der Basistabellen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>beziehen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Verknüpfung mehrerer Basistabellen geschieht bei Datenbankviews</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>über die frei definierbaren Joinbedingungen, bei allen anderen Views</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>bei Matchcodes und bei Sperrobjekten über Fremdschlüsselbeziehungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>zwischen den Tabellen.</TDLINE>
      </TLINE>
     </LINES>
    </item>
